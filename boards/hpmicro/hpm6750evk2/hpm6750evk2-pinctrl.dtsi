		group0 {
			pinmux = <HPMICRO_PINMUX(HPMICRO_PIN(HPMICRO_PORTF, 10), IOC_TYPE_IOC, 0, 24)>,
					 <HPMICRO_PINMUX(HPMICRO_PIN(HPMICRO_PORTF,  8), IOC_TYPE_IOC, 0, 24)>;
